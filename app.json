{
   "name": "hurricane",
   "version": "1.0.0.0",
   "indexHtmlPath": "index.html",
   "classpath": [
      "app"
   ],
   "overrides": [
      "overrides"
   ],
   "framework": "ext",
   "toolkit": "classic",
   "theme": "theme-crisp-2bf31ae8-211e-4f10-9ca1-4ca4e1b2104c",
   "requires": [
      "font-awesome",
      "ext-locale"
   ],
   "locale": "de",
   "fashion": {
      "inliner": {
         "enable": false
      }
   },
   "sass": {
      "namespace": "hurricane",
      "etc": [
         "sass/etc/all.scss"
      ],
      "var": [
         "sass/var/all.scss",
         "sass/var"
      ],
      "src": [
         "sass/src"
      ]
   },
   "js": [
      {
         "path": "resources/php/api.php",
         "remote": true
      },
      {
         "path": "${framework.dir}/build/ext-all-rtl-debug.js"
      },
      {
         "path": "app.js",
         "bundle": true
      }
   ],
   "css": [
      {
         "path": "${build.out.css.path}",
         "bundle": true,
         "exclude": [
            "fashion"
         ]
      }
   ],
   "loader": {
      "cache": false,
      "cacheParam": "_dc"
   },
   "production": {
      "output": {
         "appCache": {
            "enable": true,
            "path": "cache.appcache"
         }
      },
      "loader": {
         "cache": "${build.timestamp}"
      },
      "cache": {
         "enable": true
      },
      "compressor": {
         "type": "yui"
      }
   },
   "testing": {},
   "development": {
      "tags": []
   },
   "bootstrap": {
      "base": "${app.dir}",
      "microloader": "bootstrap.js",
      "css": "bootstrap.css"
   },
   "output": {
      "base": "${workspace.build.dir}/${build.environment}/${app.name}",
      "appCache": {
         "enable": false
      }
   },
   "cache": {
      "enable": false,
      "deltas": true
   },
   "appCache": {
      "cache": [
         "index.html"
      ],
      "network": [
         "*"
      ],
      "fallback": []
   },
   "resources": [
      {
         "path": "resources",
         "output": "shared"
      }
   ],
   "ignore": [
      "(^|/)CVS(/?$|/.*?$)"
   ],
   "archivePath": "archive",
   "id": "8bc1abf8-bcea-423b-86ba-535370918744"
}